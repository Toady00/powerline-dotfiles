{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "date": {
      "before": "⌚ "
    },
    "branch": {
      "before": " ",
      "args": {
        "status_colors": true
      }
    }
  },
  "segments": {
    "left": [
      {
        "name": "branch"
      }
    ],
    "right": [
      {
        "name": "date",
        "args": {
          "format": "%m/%d/%Y"
        }
      },
      {
        "name": "date",
        "args": {
          "format": "%H:%M",
          "istime": true
        }
      },
      {
        "name": "external_ip"
      }
    ]
  }
}
